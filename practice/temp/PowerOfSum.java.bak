class PowerOfSum 
{
	public static void main(String[] args) 
	{
		int a = 1234, sum = 0;
		while (a!=0)
		{
			int rem = a%10;
			int power = 1;
			for (int i=1;i<=3 ;i++ )
			{
				power = power*rem;
			}
			sum = sum+power;
			a = a/10;
		}
		System.out.println("sum of power is "+sum);
	}
}
